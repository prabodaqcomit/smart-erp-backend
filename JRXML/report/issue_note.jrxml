	<queryString><![CDATA[SELECT  t.item_name ,t.item_code, 
IFNULL((SELECT (new_qtyto_branch) FROM stock_transfer_log WHERE item_code= t.item_code and unit_price = t.unit_price and issue_number=t.issue_number and size = '0'), '') AS XS,
IFNULL((SELECT (new_qtyto_branch) FROM stock_transfer_log WHERE item_code= t.item_code and unit_price = t.unit_price and  issue_number=t.issue_number and  size = '1'), '') AS S, 
IFNULL((SELECT (new_qtyto_branch) FROM stock_transfer_log WHERE item_code= t.item_code and unit_price = t.unit_price and  issue_number=t.issue_number and   size = '2'), '') AS M, 
IFNULL((SELECT (new_qtyto_branch) FROM stock_transfer_log WHERE item_code= t.item_code and unit_price = t.unit_price and  issue_number=t.issue_number and   size = '3'), '') AS L, 
IFNULL((SELECT (new_qtyto_branch) FROM stock_transfer_log WHERE item_code= t.item_code and unit_price = t.unit_price and  issue_number=t.issue_number and   size = '4'), '') AS XL, 
IFNULL((SELECT (new_qtyto_branch) FROM stock_transfer_log WHERE item_code= t.item_code and unit_price = t.unit_price and  issue_number=t.issue_number and   size = '5'), '') AS 2XL,
IFNULL((SELECT (new_qtyto_branch) FROM stock_transfer_log WHERE item_code= t.item_code and unit_price = t.unit_price and  issue_number=t.issue_number and   size = '6'), '') AS 3XL, 
IFNULL((SELECT (new_qtyto_branch) FROM stock_transfer_log WHERE item_code= t.item_code and unit_price = t.unit_price and  issue_number=t.issue_number and   size = '7'), '') AS 4XL,
IFNULL((SELECT (new_qtyto_branch) FROM stock_transfer_log WHERE item_code= t.item_code and unit_price = t.unit_price and  issue_number=t.issue_number and   size = '8'), '') AS 5XL,
IFNULL((SELECT (new_qtyto_branch) FROM stock_transfer_log WHERE item_code= t.item_code and unit_price = t.unit_price and  issue_number=t.issue_number and   size = '9'), '') AS 6XL,
sum(t.new_qtyto_branch) as qty, t.unit_price, 
((t.unit_price)*((100-c.line_disc_limit)/100)) as cost,
(((t.unit_price)*((100-c.line_disc_limit)/100))*sum(t.new_qtyto_branch)) as value
FROM stock_transfer_log t  inner join branch_network b on (b.id=t.to_branch) 
inner join customer c on  (c.name=b.branch_name) where t.issue_number=$P{issueNumber} group by t.item_code,t.unit_price  order by t.item_code,t.unit_price asc]]></queryString>

